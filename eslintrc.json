{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:no-unsanitized/DOM"
  ],
  "plugins": [
    "prettier",
    "no-unsanitized"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
